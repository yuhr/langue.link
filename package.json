  "scripts": {
    "generate:cert": "cd ./docker/haproxy/cert && openssl req -x509 -newkey rsa:4096 -sha256 -nodes -keyout server.key -out server.crt -subj \"/CN=langue.link\" -days 3650 && cat server.{key,crt} > langue.link.pem && rm server.{key,crt}",
    "generate:icons": "svg-icon build --source icons.json --target ./src/public --name icons",
    "clear:db": "find ./docker/mongo/db/* ! -name '.gitignore' | xargs rm -rf",
    ".produce": "webpack --mode production",
    ".start:pm2": "docker-compose up",
    ".watch:chokidar": "nodemon --watch watch.config.mjs --exec 'node --experimental-modules watch.config.mjs'",
    ".watch:webpack": "nodemon --watch webpack.config.js --exec 'webpack --mode development --watch'",
    ".watch:docker": "nodemon --watch docker-compose.yml --exec 'docker-compose up'",
    "watch": "nodemon --watch .env-cmdrc --exec 'env-cmd development run-p .watch:*'"
